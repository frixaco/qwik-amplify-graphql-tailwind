{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/aws-lambda/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "serverless deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "qwik build && serverless offline",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.e2e": "playwright test",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/core": "^0.7.1",
    "@builder.io/qwik": "^1.2.10",
    "@builder.io/qwik-auth": "0.1.0",
    "@builder.io/qwik-city": "^1.2.10",
    "@modular-forms/qwik": "^0.20.2",
    "@playwright/test": "1.36.2",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-links": "7.1.1",
    "@storybook/blocks": "7.1.1",
    "@storybook/builder-vite": "7.1.1",
    "@storybook/html": "7.1.1",
    "@storybook/html-vite": "7.1.1",
    "@types/eslint": "8.44.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "autoprefixer": "^10.4.14",
    "aws-amplify": "5.3.11",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "^1.2.10",
    "graphql-request": "^6.1.0",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "serverless": "^3.32.2",
    "serverless-http": "^3.2.0",
    "serverless-offline": "^12.0.4",
    "source-map-support": "^0.5.21",
    "storybook": "7.1.1",
    "storybook-framework-qwik": "0.2.3",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.7",
    "vite-tsconfig-paths": "4.2.0"
  }
}
